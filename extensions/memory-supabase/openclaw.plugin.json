{
  "id": "memory-supabase",
  "kind": "memory",
  "uiHints": {
    "supabase.url": {
      "label": "Supabase Project URL",
      "placeholder": "https://YOUR-PROJECT.supabase.co",
      "help": "Project URL (for example https://xxxx.supabase.co)"
    },
    "supabase.serviceKey": {
      "label": "Supabase Service Role Key",
      "sensitive": true,
      "placeholder": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
      "help": "Use the service-role key so RPC functions can query pgvector tables"
    },
    "supabase.functions.search": {
      "label": "Search RPC Function",
      "placeholder": "openclaw_match_memories",
      "advanced": true
    },
    "supabase.functions.store": {
      "label": "Store RPC Function",
      "placeholder": "openclaw_store_memory",
      "advanced": true
    },
    "supabase.functions.get": {
      "label": "Get RPC Function",
      "placeholder": "openclaw_get_memory",
      "advanced": true
    },
    "supabase.functions.forget": {
      "label": "Forget RPC Function",
      "placeholder": "openclaw_forget_memory",
      "advanced": true
    },
    "supabase.functions.count": {
      "label": "Count RPC Function",
      "placeholder": "openclaw_memory_count",
      "advanced": true
    },
    "embedding.apiKey": {
      "label": "OpenAI API Key",
      "sensitive": true,
      "placeholder": "sk-proj-..."
    },
    "embedding.model": {
      "label": "Embedding Model",
      "placeholder": "text-embedding-3-small"
    },
    "autoCapture": {
      "label": "Auto-Capture"
    },
    "autoRecall": {
      "label": "Auto-Recall"
    },
    "captureMaxChars": {
      "label": "Capture Max Chars",
      "advanced": true,
      "placeholder": "500"
    },
    "maxRecallResults": {
      "label": "Max Recall Results",
      "advanced": true,
      "placeholder": "5"
    },
    "minScore": {
      "label": "Minimum Similarity",
      "advanced": true,
      "placeholder": "0.3"
    }
  },
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "supabase": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "url": {
            "type": "string"
          },
          "serviceKey": {
            "type": "string"
          },
          "functions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "search": {
                "type": "string"
              },
              "store": {
                "type": "string"
              },
              "get": {
                "type": "string"
              },
              "forget": {
                "type": "string"
              },
              "count": {
                "type": "string"
              }
            }
          }
        },
        "required": ["url", "serviceKey"]
      },
      "embedding": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "apiKey": {
            "type": "string"
          },
          "model": {
            "type": "string",
            "enum": ["text-embedding-3-small", "text-embedding-3-large"]
          }
        },
        "required": ["apiKey"]
      },
      "autoCapture": {
        "type": "boolean"
      },
      "autoRecall": {
        "type": "boolean"
      },
      "captureMaxChars": {
        "type": "number",
        "minimum": 100,
        "maximum": 10000
      },
      "maxRecallResults": {
        "type": "number",
        "minimum": 1,
        "maximum": 20
      },
      "minScore": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      }
    },
    "required": ["supabase", "embedding"]
  }
}
